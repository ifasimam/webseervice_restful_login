    function tableText(tableCell) {
        var removeClass;
        var addClass;
        var dataSort;

        if (tableCell.getAttribute("class").indexOf('sorting') >= 0) {
            if (tableCell.getAttribute("class").indexOf('sorting_asc') >= 0) {

                removeClass = 'sorting_asc';
                addClass = 'sorting_desc';
                dataSort = 'desc';

            } else if (tableCell.getAttribute("class").indexOf('sorting_desc') >= 0) {

                removeClass = 'sorting_desc';
                addClass = 'sorting_none';
                dataSort = 'none';

            } else {

                removeClass = 'sorting_none';
                addClass = 'sorting_asc';
                dataSort = 'asc';

            }
        }

        //reset all
        var tableReset = document.getElementById("dynamic-table");

        if (tableReset != null) {
            for (var i = 0; i < tableReset.rows.length; i++) {
                for (var j = 0; j < tableReset.rows[i].cells.length; j++) {

                    if (tableReset.rows[i].cells[j].className.indexOf('sorting_asc') >= 0) {
                        tableReset.rows[i].cells[j].classList.remove('sorting_asc');
                        tableReset.rows[i].cells[j].classList.add('sorting');
                        tableReset.rows[i].cells[j].dataset.sort = 'none';
                    }

                    if (tableReset.rows[i].cells[j].className.indexOf('sorting_desc') >= 0) {
                        tableReset.rows[i].cells[j].classList.remove('sorting_desc');
                        tableReset.rows[i].cells[j].classList.add('sorting');
                        tableReset.rows[i].cells[j].dataset.sort = 'none';
                    }

                }
            }
        }
        var tableReset2 = document.getElementById("dynamic-table2");

        if (tableReset2 != null) {
            for (var i = 0; i < tableReset2.rows.length; i++) {
                for (var j = 0; j < tableReset2.rows[i].cells.length; j++) {

                    if (tableReset2.rows[i].cells[j].className.indexOf('sorting_asc') >= 0) {
                        tableReset2.rows[i].cells[j].classList.remove('sorting_asc');
                        tableReset2.rows[i].cells[j].classList.add('sorting');
                        tableReset2.rows[i].cells[j].dataset.sort = 'none';
                    }

                    if (tableReset2.rows[i].cells[j].className.indexOf('sorting_desc') >= 0) {
                        tableReset2.rows[i].cells[j].classList.remove('sorting_desc');
                        tableReset2.rows[i].cells[j].classList.add('sorting');
                        tableReset2.rows[i].cells[j].dataset.sort = 'none';
                    }

                }
            }
        }

        var tableReset3 = document.getElementById("dynamic-table3");

        if (tableReset3 != null) {
            for (var i = 0; i < tableReset3.rows.length; i++) {
                for (var j = 0; j < tableReset3.rows[i].cells.length; j++) {

                    if (tableReset3.rows[i].cells[j].className.indexOf('sorting_asc') >= 0) {
                        tableReset3.rows[i].cells[j].classList.remove('sorting_asc');
                        tableReset3.rows[i].cells[j].classList.add('sorting');
                        tableReset3.rows[i].cells[j].dataset.sort = 'none';
                    }

                    if (tableReset3.rows[i].cells[j].className.indexOf('sorting_desc') >= 0) {
                        tableReset3.rows[i].cells[j].classList.remove('sorting_desc');
                        tableReset3.rows[i].cells[j].classList.add('sorting');
                        tableReset3.rows[i].cells[j].dataset.sort = 'none';
                    }

                }
            }
        }

        var tableReset4 = document.getElementById("dynamic-table4");

        if (tableReset4 != null) {
            for (var i = 0; i < tableReset4.rows.length; i++) {
                for (var j = 0; j < tableReset4.rows[i].cells.length; j++) {

                    if (tableReset4.rows[i].cells[j].className.indexOf('sorting_asc') >= 0) {
                        tableReset4.rows[i].cells[j].classList.remove('sorting_asc');
                        tableReset4.rows[i].cells[j].classList.add('sorting');
                        tableReset4.rows[i].cells[j].dataset.sort = 'none';
                    }

                    if (tableReset4.rows[i].cells[j].className.indexOf('sorting_desc') >= 0) {
                        tableReset4.rows[i].cells[j].classList.remove('sorting_desc');
                        tableReset4.rows[i].cells[j].classList.add('sorting');
                        tableReset4.rows[i].cells[j].dataset.sort = 'none';
                    }

                }
            }
        }
        // end

        if (tableCell.getAttribute("class").indexOf('sorting') >= 0) {
            tableCell.classList.remove(removeClass);
            tableCell.classList.add(addClass);
            tableCell.dataset.sort = dataSort;

            $.ajax({
                type: "GET",
                url: '@Html.Toyota().Page.GetActionUrl("GetPRInquirySort")',
                data: {
                    field: tableCell.dataset.field,
                    sort: dataSort
                },
                contentType: "application/json; charset=utf-8",
                success: function (result) {
                    $("#tblScroll")
                            .find('tbody')
                            .html(
                                result
                            );
                    $("#tblScroll2")
                    .find('tbody')
                    .html(
                        result
                    );
                    $("#tblScroll3")
                  .find('tbody')
                  .html(
                      result
                  );
                    $("#tblScroll4")
                  .find('tbody')
                  .html(
                      result
                  );
                }
            });
        }
    }
</script>

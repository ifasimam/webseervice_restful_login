<div id="delete-conf" class="modal fade"> 
    <div class="modal-dialog modal-md" style="width: 400px;">
        <div class="modal-content">
            <div class="modal-header">
                <div class="close" style="opacity: 1 !important; margin-top: -7px;">
                    <span aria-hidden="true"><img src="@Url.Content("~/Content/Bootstrap/img/question.png")" class="modal-icon" /></span>
                    <span class="sr-only">Close</span>
                </div>
                <h4 class="modal-title" id="popup-title">Confirmation</h4>
            </div>

            <div class="modal-body">
                <div class="modal-body">
                    <input type="text" id="PARAMDELETE" value=""/>
                    <div class="row" style="padding-left: 10px;">
                        <div id="MsgDeleteModal">&nbsp;</div>
                    </div>
                    <div class="row" style="text-align: right;padding-right: 10px; margin-top: 10px;">
                        <button type="button" class="btn btn-primary btn-xs" data-dismiss="modal" style="width: 60px;" id="btnDelete" onclick="javascript:onDeleteData()">Yes</button>
                        <button class="btn btn-danger btn-xs" data-dismiss="modal" style="width: 60px;" id="btnCanceldel">No</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

